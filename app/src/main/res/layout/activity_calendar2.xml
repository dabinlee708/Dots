    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.projects.ui.tldr.CalendarActivity"
        android:orientation="vertical"
        android:background="#eeeeee">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:padding="10dp"
            android:visibility="gone"
            android:id="@+id/toolbar">


            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttonselected"
                android:paddingTop="8dp"
                android:text="ALL"
                android:textColor="#fff"
                android:layout_marginRight="5dp"
                android:id="@+id/ALL"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttondeselected"
                android:paddingTop="8dp"
                android:text="ISTD"
                android:textColor="#fff"
                android:layout_marginRight="5dp"
                android:id="@+id/ISTD"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttondeselected"
                android:paddingTop="8dp"
                android:text="EPD"
                android:textColor="#fff"
                android:layout_marginRight="5dp"
                android:id="@+id/EPD"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttondeselected"
                android:paddingTop="8dp"
                android:text="ASD"
                android:textColor="#fff"
                android:layout_marginRight="5dp"
                android:id="@+id/ASD"/>

            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttondeselected"
                android:paddingTop="8dp"
                android:text="ESD"
                android:textColor="#fff"
                android:layout_marginRight="5dp"
                android:id="@+id/ESD"/>
            <Button
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="@drawable/circlebuttondeselected"
                android:paddingTop="8dp"
                android:text="5th"
                android:textColor="#fff"
                android:id="@+id/fifth"/>
        </LinearLayout>
        <ImageView
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/caret"
            android:layout_gravity="center_horizontal"
            android:gravity="center"
            android:src="@drawable/caretdown"
            android:onClick="onClick"
            android:clickable="true"
            android:layout_marginTop="5dp" />

        <GridLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TextView
                android:layout_column="1"
                android:text="TIME"
                android:layout_row="1"
                android:padding="10dp"
                android:layout_gravity="left|top"
                android:textStyle="bold"/>

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="2"
                android:text="0800"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="2"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="3"
                android:text="1000"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="4"
                android:text="1100"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="5"
                android:text="1200"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="6"
                android:text="1300"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="7"
                android:text="1400"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="8"
                android:text="1500"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="9"
                android:text="1600"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="10"
                android:text="1700"
                android:padding="10dp" />

            <TextView
                android:layout_column="1"
                android:layout_gravity="left|top"
                android:layout_row="11"
                android:text="1800"
                android:padding="10dp" />

            <TextView
                android:layout_column="2"
                android:layout_gravity="start|top"
                android:layout_row="1"
                android:text="MY EVENTS"
                android:padding="10dp"
                android:textStyle="bold"/>
            <!--<TextView-->
                <!--android:layout_column="2"-->
                <!--android:layout_row="3"-->
                <!--android:text="UI"-->
                <!--android:background="#FFA500"-->
                <!--android:padding="10dp"-->
                <!--android:layout_columnSpan="1"-->
                <!--android:layout_rowSpan="2"-->
                <!--android:layout_gravity="center_horizontal|fill"-->
                <!--android:onClick="onClick"-->
                <!--android:clickable="true"-->
                <!--android:gravity="center"-->
                <!--android:id="@+id/UI"/>-->

            <TextView
                android:layout_column="3"
                android:layout_row="3"
                android:background="#222d2d2d"
                android:padding="10dp"
                android:layout_columnSpan="3"
                android:layout_rowSpan="2"
                android:layout_gravity="center_horizontal|fill"
                android:gravity="center" />

            <TextView
                android:layout_column="2"
                android:layout_row="5"
                android:text="Capstone"
                android:background="#FF0000"
                android:padding="10dp"
                android:layout_columnSpan="1"
                android:layout_rowSpan="1"
                android:layout_gravity="fill"
                android:onClick="onClick"
                android:gravity="center"
                android:clickable="true"
                android:id="@+id/Security"/>

            <TextView
                android:layout_column="3"
                android:layout_row="5"
                android:background="#222d2d2d"
                android:padding="10dp"
                android:layout_columnSpan="3"
                android:layout_gravity="center_horizontal|fill"
                android:gravity="center"
                android:layout_height="wrap_content" />

            <TextView
                android:layout_column="2"
                android:layout_row="8"
                android:text="EM"
                android:background="#fff000"
                android:padding="10dp"
                android:layout_columnSpan="1"
                android:layout_rowSpan="1"
                android:layout_gravity="fill"
                android:gravity="center"
                android:onClick="onClick"
                android:clickable="true"
                android:id="@+id/HASS"/>

            <TextView
                android:layout_column="3"
                android:layout_row="8"
                android:background="#222d2d2d"
                android:padding="10dp"
                android:layout_columnSpan="3"
                android:layout_gravity="center_horizontal|fill"
                android:gravity="center"
                android:layout_height="wrap_content" />

            <TextView
                android:layout_column="3"
                android:gravity="center"
                android:layout_columnSpan="3"
                android:layout_gravity="fill_horizontal"
                android:layout_row="1"
                android:text="NEW EVENTS"
                android:padding="10dp"
                android:textStyle="bold"/>

            <TextView
                android:layout_column="3"
                android:layout_row="3"
                android:text="MB"
                android:background="#1114e1"
                android:padding="10dp"
                android:layout_columnSpan="1"
                android:layout_rowSpan="1"
                android:layout_gravity="fill"
                android:gravity="center"
                android:onClick="onClick"
                android:clickable="true"
                android:id="@+id/musicbox"/>

            <!--<TextView-->
                <!--android:layout_column="4"-->
                <!--android:layout_row="5"-->
                <!--android:text="CT"-->
                <!--android:background="#e111aa"-->
                <!--android:padding="10dp"-->
                <!--android:layout_columnSpan="1"-->
                <!--android:layout_rowSpan="2"-->
                <!--android:layout_gravity="fill"-->
                <!--android:gravity="center"-->
                <!--android:onClick="onClick"-->
                <!--android:clickable="true"-->
                <!--android:id="@+id/chill"/>-->

            <TextView
                android:layout_column="5"
                android:layout_row="6"
                android:text="ST"
                android:background="#37e111"
                android:padding="10dp"
                android:layout_columnSpan="1"
                android:layout_rowSpan="4"
                android:layout_gravity="fill"
                android:gravity="center"
                android:onClick="onClick"
                android:clickable="true"
                android:id="@+id/study"/>

        </GridLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center_horizontal"
            android:padding="10dp">

            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text=""
                android:padding="10dp"
                android:visibility="gone"
                android:id="@+id/selected"/>

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="View Details"
                android:visibility="gone"
                android:id="@+id/button" />

        </LinearLayout>

    </LinearLayout>

